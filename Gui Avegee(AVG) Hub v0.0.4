RunButton.MouseButton1Click:Connect(function()
	local list = Showcmd:GetChildren()
	for i = 1,#list do
		local temp = list[i]
		if temp.ClassName ~= "UIGridLayout" then
			local Value = temp:FindFirstChild("Value")
			if Value ~= nil then
				if temp.Name == "Wait" then
					wait(Value.Value)
				end
			else
				local posX = temp:FindFirstChild("posX")
				local posY = temp:FindFirstChild("posY")
				local posZ = temp:FindFirstChild("posZ")
				if posX ~= nil and posY ~= nil and posZ ~= nil then
					if temp.Name == "Teleport" then
						local Torso = game.Players.LocalPlayer.Character.Humanoid.Torso
						Torso.CFrame = CFrame.new(posX.Value,posY.Value,posZ.Value)
					end
				end
			end
		end
	end
end)
