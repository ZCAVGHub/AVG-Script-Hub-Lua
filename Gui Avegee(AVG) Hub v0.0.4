function anchor(Button)
	local cha = game:GetService("Players").LocalPlayer.Character
	if cha.HumanoidRootPart.Anchored == false then
		cha.HumanoidRootPart.Anchored = true
		Button.Text = "   🟩 ⚓️ Anchor Character"
	else
		cha.HumanoidRootPart.Anchored = false
		Button.Text = "   🟥 ⚓️ Anchor Character"
	end
end
function fly(speed)
	repeat wait() until game.Players.LocalPlayer.Character
	local plr = game:GetService("Players").LocalPlayer
	local character = plr.Character
	local humanoid = character.Humanoid
	local humanoidrootPart = character.HumanoidRootPart
	local mouse = plr:GetMouse()

	local enable = false

	mouse.KeyDown:Connect(function(key)
		if key == "f" then
			enable = true
			local bv = Instance.new("BodyVelocity", humanoidrootPart)
			bv.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
			while enable == true do
				bv.Velocity = mouse.Hit.LookVector * speed
				humanoid.Sit = true
				wait()
			end
		end
	end)
	mouse.KeyUp:Connect(function(key)
		if key == "f" then
			if enable == true then
				enable = false
				humanoid.Sit = false
				humanoidrootPart:FindFirstChildOfClass("BodyVelocity"):Destroy()
			end
		end
	end)
end
function sit()
	local cha = game:GetService("Players").LocalPlayer.Character
	if cha.Humanoid.Sit == false then
		cha.Humanoid.Sit = true
	else
		cha.Humanoid.Sit = false
	end
end
local function getPlayerFromCharacter(character)
	for _, player in pairs(game:GetService("Players"):GetPlayers()) do
		if player.Character == character then
			return player
		end
	end
end
function createwaypoint(name,color)
	if game.Workspace:FindFirstChild(name) == nil then
		local part = Instance.new("Part",game.Workspace)
		part.Name = name
		part.Anchored = true
		part.Transparency = 1
		part.CanCollide = false
		part.CFrame = game:GetService("Players").LocalPlayer.Character.Humanoid.Torso.CFrame

		local BillboardGui = Instance.new("BillboardGui", part)
		BillboardGui.Size = UDim2.new(0,10,0,10)
		BillboardGui.AlwaysOnTop = true
		local frame = Instance.new("Frame",BillboardGui)
		frame.Size = UDim2.new(0,10,0,10)
		frame.Rotation = 45
		frame.BackgroundTransparency = 0.5
		frame.BackgroundColor3 = color
	end
end
function removewaypoint(name)
	game.Workspace:FindFirstChild(name):Destroy()
end
function teleporttowaypoint(name)
	game:GetService("Players").LocalPlayer.Character.Humanoid.Torso.CFrame = game.Workspace:FindFirstChild(name).CFrame
end
function create(parent,color,name)
	local BillboardGui = Instance.new("BillboardGui", parent)
	BillboardGui.Size = UDim2.new(0,200,0,84)
	BillboardGui.AlwaysOnTop = true
	local Frame = Instance.new("Frame",BillboardGui)
	Frame.Size = UDim2.new(0,40,0,60)
	Frame.Position = UDim2.new(0,80,0,2)
	local UIStroke = Instance.new("UIStroke", Frame)
	UIStroke.Thickness = 2
	UIStroke.Color = color
	local UIGradient = Instance.new("UIGradient", Frame)
	UIGradient.Transparency = NumberSequence.new{
		NumberSequenceKeypoint.new(0,1),
		NumberSequenceKeypoint.new(.5,1),
		NumberSequenceKeypoint.new(1,1)
	}
	local TextLabel = Instance.new("TextLabel",BillboardGui)
	TextLabel.Text = name
	TextLabel.Size = UDim2.new(0,200,0,20)
	TextLabel.Position = UDim2.new(0,0,0,62)
	TextLabel.BackgroundTransparency = 1
	TextLabel.TextColor3 = color
	TextLabel.TextSize = 14
	TextLabel.Font = Enum.Font.Code
	return BillboardGui
end

function showallplr(db)
	if db.Value == true then
		local list = db:GetChildren()
		for i = 1, #list do
			local object = list[i].Value
			object:Destroy()
			list[i]:Destroy()
		end
	else
		local child = game:GetService("Players").LocalPlayer.Character.Parent:GetChildren()
		local temp = nil
		for i = 1,#child do
			temp = child[i]
			if temp.ClassName == "Model" then
				local human = temp:FindFirstChild("Humanoid")
				if human ~= nil and temp.Name ~= game:GetService("Players").LocalPlayer.Name then
					local find = temp.HumanoidRootPart:FindFirstChild("BillboardGui")
					if find == nil then
						local tag = Instance.new("ObjectValue",db)
						local BillboardGui = create(temp.HumanoidRootPart,randomcolor[math.random(1,#randomcolor)],temp.Name)
						tag.Value = BillboardGui
					end
				end
			end
		end
	end
end
function showallplrarsenal(db)
	if db.Value == true then
		local list = db:GetChildren()
		for i = 1, #list do
			local object = list[i].Value
			if object ~= nil then
				object:Destroy()
				list[i]:Destroy()
			else
				list[i]:Destroy()
			end
		end
	else
		local child = game:GetService("Players"):GetChildren()
		local temp = nil
		for i = 1,#child do
			temp = child[i]
			if temp.ClassName == "Player" then
				if temp.Team ~= game.Players.LocalPlayer.Team then
					local human = temp.Character:FindFirstChild("Humanoid")
					if human ~= nil and temp.Name ~= game:GetService("Players").LocalPlayer.Name then
						local find = temp.Character.HumanoidRootPart:FindFirstChild("BillboardGui")
						if find == nil then
							local tag = Instance.new("ObjectValue",db)
							local BillboardGui = create(temp.Character.HumanoidRootPart,randomcolor[math.random(1,#randomcolor)],temp.Name)
							tag.Value = BillboardGui
						end
					end
				end
			end
		end
	end
end
