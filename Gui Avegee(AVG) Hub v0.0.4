function cancollidemaparsenal(db)
	if db.Value == false then
		local Transparency = 0
		local CanCollide = true
		local child = game.Workspace.Map.Geometry:GetChildren()
		local temp = nil
		for i = 1,#child do
			temp = child[i]
			if temp.ClassName == "Part" then
				temp.Transparency = Transparency
				temp.CanCollide = CanCollide
			end
			if temp.ClassName == "Model" then
				local inM = temp:GetChildren()
				for o = 1, #inM do
					local part = inM[o]
					if part.ClassName == "Part" then
						part.Transparency = Transparency
						part.CanCollide = CanCollide
					end
				end
			end
		end
		local part = game.Workspace:FindFirstChild("NewPartInTheWorldforAVGHubCreate")
		if part then
			part:Destroy()
		end
	else
		local Transparency = 0.7
		local CanCollide = false
		local child = game.Workspace.Map.Geometry:GetChildren()
		local temp = nil
		for i = 1,#child do
			temp = child[i]
			if temp.ClassName == "Part" then
				temp.Transparency = Transparency
				temp.CanCollide = CanCollide
			end
			if temp.ClassName == "Model" then
				local inM = temp:GetChildren()
				for o = 1, #inM do
					local part = inM[o]
					if part.ClassName == "Part" then
						part.Transparency = Transparency
						part.CanCollide = CanCollide
					end
				end
			end
		end
		local cha = game.Players.LocalPlayer.Character
		local part = Instance.new("Part",game.Workspace)
		part.Name = "NewPartInTheWorldforAVGHubCreate"
		part.Anchored = true
		part.Size = Vector3.new(5000,1,5000)
		part.CFrame = cha.HumanoidRootPart.CFrame * CFrame.new(0,-4,0)
	end
end
