		ping = "🟧"
	elseif GetPing() > 50 then
		ping = "🟨"
	elseif GetPing() > 10 then
		ping = "🟩"
	elseif GetPing() < 10 then
		ping = "🟪"
	end
	TextName.Text = "AVG Script Hub    v0.0.4    "..math.floor(game.Workspace:GetRealPhysicsFPS() + 0.5).." Fps  "..fps.." Ping "..GetPing().." ms "..ping
end
