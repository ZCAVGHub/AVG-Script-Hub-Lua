function close(ScreenGui)
	ScreenGui:Destroy()
end
function small(frame,frame2,frame3)
	if frame.Visible == true then
		frame.Visible = false
	else
		frame.Visible = true
		frame2.BackgroundTransparency = 0
		frame3.TextTransparency = 0
	end
end
function onOff(frame,frame2,frame3)
	if frame.Visible == true then
		frame.Visible = false
		frame2.BackgroundTransparency = 1
		frame3.TextTransparency = 1
	else
		frame.Visible = true
		frame2.BackgroundTransparency = 0
		frame3.TextTransparency = 0
	end
end
function openframe(frame)
	local list = frame.Parent:GetChildren()
	for i = 1,#list do
		local temp = list[i]
		if temp ~= frame then
			if temp.Visible == true then
				temp.Visible = false
			end
		end
	end
	frame.Visible = true
end
